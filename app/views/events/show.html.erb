<section class="event-details">
  
  <div class="details">
    <h1><%= @event.name %></h1>
    <h2>
       <%= price(@event) %>
    </h2>
    <h3>
      <%= @gievent.starts_at.strftime("%B %d at %I:%M %P") %> in <%= @event.location %>
    </h3>
    <p>
      <%= @event.description %>
    </p>

    

  </div>
</section>
